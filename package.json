{
  "name": "gw-core",
  "version": "0.0.0",
  "description": "Basic Roguelike functionality",
  "main": "dist/gw-core.js",
  "browser": "dist/gw-core.min.js",
  "module": "dist/gw-core.js",
  "types": "dist/gw-core.d.ts",
  "dependencies": {},
  "devDependencies": {
    "@babel/types": "^7.12.10",
    "@types/jest": "^26.0.19",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "prettier": "^2.2.1",
    "rollup": "^2.35.0",
    "rollup-plugin-dts": "^2.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "compile": "tsc --build tsconfig.json",
    "bundle": "rollup -c",
    "copy": "cp dist/gw-core.min.js docs/js",
    "build": "npm run compile && npm run bundle && npm run copy",
    "serve": "python -m SimpleHTTPServer",
    "test": "jest",
    "posttest": "npm run build",
    "test-debug": "node --inspect-brk node_modules/.bin/jest --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/funnisimo/gw-core.git"
  },
  "keywords": [
    "Roguelike"
  ],
